


#using conditionals inside loops
my_list = [10, 35, 40, 50, 60]
print(my_list)
for element in my_list:
    if element > 40:
        print(f"{element} is greater than 40")
    elif element == 40:
        print(f"{element} is equal to 40")
    else:
        print(f"{element} is less than 40")
        


